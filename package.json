{
  "name": "ngx-echarts-demo",
  "version": "3.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build ngx-echarts",
    "build:prod": "ng build ngx-echarts --prod",
    "update:md": "cpr README.md dist/ngx-echarts/README.md -o",
    "dist": "run-s build:prod update:md",
    "release": "run-s build:prod update:md build:doc",
    "demo": "run-s build:lib start",
    "build:doc": "run-s build compodoc",
    "compodoc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0",
    "@angular/common": "^6.0.0",
    "@angular/compiler": "^6.0.0",
    "@angular/core": "^6.0.0",
    "@angular/forms": "^6.0.0",
    "@angular/http": "^6.0.0",
    "@angular/platform-browser": "^6.0.0",
    "@angular/platform-browser-dynamic": "^6.0.0",
    "@angular/router": "^6.0.0",
    "core-js": "^2.5.4",
    "echarts": "^4.1.0",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^2.0.5",
    "ngx-markdown": "^1.6.0",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.0",
    "@angular-devkit/build-ng-packagr": "~0.6.0",
    "@angular/cli": "~6.0.0",
    "@angular/compiler-cli": "^6.0.0",
    "@angular/language-service": "^6.0.0",
    "@compodoc/compodoc": "^1.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.1",
    "codelyzer": "~4.2.1",
    "cpr": "^3.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0-rc.2",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
    "raw-loader": "^0.5.1",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
