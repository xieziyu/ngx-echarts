{
  "name": "ngx-echarts-demo",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng-packagr",
    "link:lib": "rimraf node_modules/ngx-echarts && cpr dist/ngx-echarts node_modules/ngx-echarts",
    "update:md": "cpr README.md dist/ngx-echarts/README.md -o",
    "dist": "run-s build:lib update:md",
    "release": "run-s build:lib update:md link:lib build:doc",
    "demo": "run-s build:lib link:lib start",
    "build:doc": "run-s build compodoc",
    "compodoc": "compodoc -p lib/src/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p lib/src/tsconfig.lib.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.0",
    "@angular/common": "5.2.0",
    "@angular/compiler": "5.2.0",
    "@angular/core": "5.2.0",
    "@angular/forms": "5.2.0",
    "@angular/http": "5.2.0",
    "@angular/platform-browser": "5.2.0",
    "@angular/platform-browser-dynamic": "5.2.0",
    "@angular/router": "5.2.0",
    "core-js": "2.4.1",
    "echarts": "^3.8.5",
    "global": "^4.3.2",
    "rxjs": "5.5.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.6.3",
    "@angular/compiler-cli": "5.2.0",
    "@angular/language-service": "5.2.0",
    "@compodoc/compodoc": "^1.0.4",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "@types/prismjs": "^1.6.5",
    "bootstrap": "4.0.0-beta.3",
    "codelyzer": "4.0.2",
    "cpr": "^3.0.1",
    "font-awesome": "^4.7.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^1.6.0",
    "ngx-bootstrap": "^2.0.0-beta.8",
    "ngx-markdown": "1.5.0",
    "npm-run-all": "^4.1.2",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "ts-node": "~3.2.0",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
