{
  "name": "ngx-echarts-demo",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build ngx-echarts",
    "build:prod": "ng build ngx-echarts --prod",
    "update:md": "cpr README.md dist/ngx-echarts/README.md -o",
    "dist": "run-s build:prod update:md",
    "release": "run-s build:prod update:md build:doc",
    "demo": "run-s build:lib start",
    "build:doc": "run-s build compodoc",
    "compodoc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "angular2-draggable": "2.1.2",
    "core-js": "^2.5.4",
    "echarts": "^4.1.0",
    "echarts-gl": "^1.1.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^2.0.5",
    "ngx-markdown": "^7.0.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.1.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@compodoc/compodoc": "^1.1.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.1",
    "codelyzer": "~4.5.0",
    "cpr": "^3.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "raw-loader": "^0.5.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
