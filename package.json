{
  "name": "ngx-echarts-demo",
  "version": "4.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4202",
    "build": "ng build --prod --base-href ./",
    "test": "npm run lint",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build ngx-echarts",
    "build:prod": "ng build ngx-echarts --prod",
    "update:md": "cpr README.md dist/ngx-echarts/README.md -o",
    "dist": "run-s build:prod update:md",
    "release": "run-s build:prod update:md build:doc",
    "demo": "run-s build:lib start",
    "build:doc": "run-s build compodoc",
    "compodoc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/ngx-echarts/tsconfig.lib.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.1",
    "@angular/common": "~8.1.1",
    "@angular/compiler": "~8.1.1",
    "@angular/core": "~8.1.1",
    "@angular/forms": "~8.1.1",
    "@angular/platform-browser": "~8.1.1",
    "@angular/platform-browser-dynamic": "~8.1.1",
    "@angular/router": "~8.1.1",
    "angular2-draggable": "^2.3.2",
    "core-js": "^3.1.4",
    "echarts": "^4.2.1",
    "echarts-gl": "^1.1.1",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "^5.1.0",
    "ngx-markdown": "^8.1.0",
    "rxjs": "~6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.1",
    "@angular-devkit/build-ng-packagr": "~0.801.1",
    "@angular/cli": "~8.1.1",
    "@angular/compiler-cli": "~8.1.1",
    "@angular/language-service": "~8.1.1",
    "@compodoc/compodoc": "^1.1.9",
    "@types/echarts": "^4.1.9",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.6.2",
    "bootstrap": "^4.1.1",
    "codelyzer": "^5.1.0",
    "cpr": "^3.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.3.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "raw-loader": "^0.5.1",
    "ts-node": "^8.3.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.3"
  }
}
